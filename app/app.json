{
  "expo": {
    "name": "Quil",
    "slug": "quil",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/AppIcons/playstore.png",
    "scheme": "quil-app",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/AppIcons/playstore.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0e27"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "app.sddion.quil",
      "icon": "./assets/images/AppIcons/appstore.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/AppIcons/android/mipmap-xxxhdpi/ic_launcher.png",
        "backgroundColor": "#0a0e27"
      },
      "package": "app.sddion.quil",
      "permissions": [
        "android.permission.VIBRATE",
        "android.permission.INTERNET",
        "android.permission.REQUEST_INSTALL_PACKAGES"
      ],
      "softwareKeyboardLayoutMode": "pan"
    },

    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://meetquil.vercel.app/"
        }
      ],
       [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true,
            "enableMinifyInReleaseBuilds": true,
            "minSdkVersion": 26,
            "enableSeparateBuildPerCPUArchitecture": true
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "jsEngine": "hermes",
    "runtimeVersion": "2.1.1"
  }
}
